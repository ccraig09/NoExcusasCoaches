{
  "expo": {
    "name": "NoExcusasCoaches",
    "slug": "NoExcusasCoaches",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon":  "./assets/icon-noexlogo.png",
    "splash": {
      "image": "./assets/icon-noexlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-noexlogo.png",
          "color": "blue",
          "mode": "production"
        }
      ]
    ],
    "android": {
      "useNextNotificationsApi" :true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
