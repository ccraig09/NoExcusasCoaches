{
  "expo": {
    "name": "NoExcusasCoaches",
    "slug": "NoExcusasCoaches",
    "version": "1.0.0",

    "icon": "./assets/icon-noexlogo.png",
    "splash": {
      "image": "./assets/icon-noexlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#CD9A21"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ccraig09.NoExcusasCoaches"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-noexlogo.png",
          "color": "blue",
          "mode": "production"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
